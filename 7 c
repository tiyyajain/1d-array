#include <iostream>
#include <string>
using namespace std;

int main()
{
    string str1,str2;
    cout<<" Enter the first string: ";
    getline(cin,str1);
    cout<<" Enter the second string: ";
    getline(cin,str2);

    cout<<" Concatenated string is: "<<str1+str2;
    return 0;
    
}

output
Enter the first string: hello
 Enter the second string: world
 Concatenated string is: helloworld
